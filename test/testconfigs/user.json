{
    "model": "User",
    "batchSize": 300,
    "indexOnStart": true,
    "forceIndexOnStart": false,
    "forceDeleteOnStart": true,
    "dbQuery": {
        "collection": "User",
        "fields": [
            "_id",
            "firstName",
            {
                "field": "Age",
                "value": "age"
            }
        ]
    },
    "indexSettings": {
        "settings": {
            "analysis": {
                "filter": {
                    "custom_edgegram": {
                        "type": "edgeNGram",
                        "min_gram": "1",
                        "max_gram": "100"
                    },
                    "wordsdelimeter": {
                        "catenate_words": true,
                        "catenate_all": true,
                        "preserve_original": true,
                        "type": "word_delimiter",
                        "catenate_numbers": true
                    }
                },
                "analyzer": {
                    "autocomplete_analyzer": {
                        "filter": [
                            "lowercase",
                            "wordsdelimeter",
                            "asciifolding"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "autosearch": {
                        "filter": [
                            "lowercase",
                            "wordsdelimeter",
                            "asciifolding"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    }
                }
            }
        },
        "mappings": {
            "properties": {
                "name": {
                    "type": "text",
                    "analyzer": "autocomplete_analyzer",
                    "search_analyzer": "autosearch"
                },
                "recentJobTitle": {
                    "type": "text",
                    "analyzer": "autocomplete_analyzer",
                    "search_analyzer": "autosearch"
                },
                "recentCompany": {
                    "type": "text",
                    "analyzer": "autocomplete_analyzer",
                    "search_analyzer": "autosearch"
                },
                "email": {
                    "type": "text",
                    "analyzer": "autocomplete_analyzer",
                    "search_analyzer": "autosearch",
                    "fields": {
                        "keyword": {
                            "type": "keyword"
                        }
                    }
                }
            }
        }
    }
}