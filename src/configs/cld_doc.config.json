{
    "collection": "CldDoc",
    "doc_type": "CldDoc",
    "index_name": "cld_docs_dev",
    "index_on_start": false,
    "force_delete": false,
    "batch_size": 500,
    "aggregation_pipeline": [
        {
            "$lookup": {
                "from": "CldField",
                "localField": "cldFieldLanguageId",
                "foreignField": "_id",
                "as": "cldFieldLanguage"
            }
        },
        {
            "$lookup": {
                "from": "LtdDoc",
                "localField": "ltdDocId",
                "foreignField": "_id",
                "as": "ltdDoc",
                "pipeline": [
                    {
                        "$lookup": {
                            "from": "Language",
                            "localField": "languageIds",
                            "foreignField": "_id",
                            "as": "languages",
                            "pipeline": [
                                {
                                    "$project": {
                                        "name": 1,
                                        "langISO": 1
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "$lookup": {
                            "from": "ICCSituation",
                            "localField": "iccSituationIds",
                            "foreignField": "_id",
                            "as": "iccSituations",
                            "pipeline": [
                                {
                                    "$project": {
                                        "title": 1
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "$lookup": {
                            "from": "PhaseOfCase",
                            "localField": "phaseOfCaseIds",
                            "foreignField": "_id",
                            "as": "phaseOfCases",
                            "pipeline": [
                                {
                                    "$project": {
                                        "title": 1,
                                        "title_trans": 1
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "$project": {
                            "id": 1,
                            "doc_type": 1,
                            "content_type": 1,
                            "documentType": 1,
                            "created": 1,
                            "updated": 1,
                            "dateCreated": 1,
                            "phaseOfCases": 1,
                            "source": 1,
                            "title": 1,
                            "cld": 1,
                            "_decision": 1,
                            "caseNumber": 1,
                            "languages": 1,
                            "numberOfPages": 1,
                            "interlocutory": 1,
                            "presidingJudge": 1,
                            "caseName_en": 1,
                            "iccCaseCited": 1,
                            "slug": 1,
                            "proxySlugs": 1,
                            "pdfURL": 1,
                            "iccSituations": 1,
                            "iccSituationIds": 1,
                            "phaseOfCaseIds": 1,
                            "languageIds": 1,
                            "cldJudges": 1,
                            "typeOfDecisionEN": 1,
                            "typeOfDecisionFR": 1,
                            "documentTypeEN": 1,
                            "documentTypeFR": 1,
                            "documentSourceEN": 1,
                            "documentSourceFR": 1,
                            "newVersionAvailable": 1,
                            "deleted": 1
                        }
                    }
                ]
            }
        },
        {
            "$project": {
                "Title": 1,
                "id": 1,
                "Appeal Doc ID": 1,
                "Keyword": 1,
                "keywordIds": 1,
                "created": 1,
                "legalFinding": 1,
                "ltdDoc": 1,
                "Finding": 1,
                "ID": 1,
                "deleted": 1,
                "editorCopy": 1,
                "Confidentiality": 1,
                "progress": 1,
                "Source": 1,
                "Importance01": 1,
                "dateCreated": 1,
                "dateModified": 1,
                "Case/Document number": 1,
                "Document Number": 1,
                "situationCase": 1,
                "cldFieldLanguageId": 1,
                "cldFieldLanguage": 1
            }
        }
    ],
    "index_params": {
        "settings": {
            "index.number_of_shards": 4,
            "index.number_of_replicas": 2,
            "analysis": {
                "filter": {
                    "custom_edgegram": {
                        "type": "edgeNGram",
                        "min_gram": "4",
                        "max_gram": "100"
                    },
                    "wordsdelimeter": {
                        "catenate_words": true,
                        "catenate_all": true,
                        "preserve_original": true,
                        "type": "word_delimiter",
                        "catenate_numbers": true
                    },
                    "english_stop_words": {
                        "type": "stop",
                        "stopwords": "_english_",
                        "ignore_case": true
                    }
                },
                "analyzer": {
                    "autocomplete_analyzer": {
                        "filter": [
                            "lowercase",
                            "wordsdelimeter"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "autosearch": {
                        "filter": [
                            "lowercase"
                        ],
                        "type": "custom",
                        "tokenizer": "whitespace"
                    },
                    "normalize": {
                        "filter": [
                            "lowercase",
                            "asciifolding"
                        ],
                        "type": "custom",
                        "tokenizer": "keyword"
                    }
                }
            }
        },
        "mappings": {
            "dynamic": false,
            "properties": {
                "Title": {
                    "type": "text",
                    "analyzer": "autocomplete_analyzer",
                    "search_analyzer": "autosearch",
                    "fielddata": true,
                    "fields": {
